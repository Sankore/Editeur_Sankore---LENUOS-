<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:v="urn:import:com.paraschool.editor.client.view"
	xmlns:h="urn:import:com.paraschool.htmllist.client">

	<ui:with type="com.paraschool.editor.client.view.FiltersView.Resources"
		field="res"></ui:with>

	<g:HTMLPanel styleName="{res.css.filters}">
		<div>
			<img src="{res.separator.getURL}" alt="" title="" />
		</div>
		<h:HTMLList ui:field="list"/>
	</g:HTMLPanel>
</ui:UiBinder>