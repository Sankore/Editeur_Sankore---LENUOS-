<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:app="urn:import:com.paraschool.editor.client.view">
	
	<ui:with type="com.paraschool.editor.client.view.PageView.Resources" field="res"></ui:with>
	
	<g:AbsolutePanel ui:field="container" styleName="{res.css.page}">
		<g:FlowPanel ui:field="display" styleName="{res.css.display}">
			<g:HTML styleName="{res.css.displayHead}"></g:HTML>
			<g:FlowPanel ui:field="subview"></g:FlowPanel>
			
			<app:PageContentPanel ui:field="area" styleName="{res.css.area}">
			</app:PageContentPanel>
			<g:HTML styleName="{res.css.displayBottom}"></g:HTML>
		</g:FlowPanel>
	</g:AbsolutePanel>
</ui:UiBinder> 