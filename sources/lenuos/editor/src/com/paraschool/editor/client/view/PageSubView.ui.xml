<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with type="com.paraschool.editor.client.view.PageSubView.Resources" field="res"></ui:with>

	<g:HTMLPanel styleName="{res.css.subView}">
		<div class="{res.css.subViewHead}"></div>
		<g:FlowPanel ui:field="content" styleName="{res.css.subViewContent}">
			<g:Image ui:field="loadingIndicator" url='{res.loading.getURL}' styleName="{res.css.viewLoading}"></g:Image>
		</g:FlowPanel>
		<div class="{res.css.subViewBottom}"></div>
		<div class="{res.appCss.clear}"></div>
	</g:HTMLPanel>
</ui:UiBinder> 