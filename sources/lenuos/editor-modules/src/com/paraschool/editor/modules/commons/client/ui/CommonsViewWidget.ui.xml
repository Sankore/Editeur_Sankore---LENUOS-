<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:c="urn:import:com.paraschool.editor.modules.commons.client.ui">
	
	<ui:style field="css" type="com.paraschool.editor.modules.commons.client.ui.CommonsViewWidget.CssResource">
		@external head, content, switched, has-thumbnail;
		.head{}
		.content{}
		.switched{}
		.has-thumbnail{}
	</ui:style>
	
	<g:FlowPanel styleName="interactivity">
		<g:FormPanel ui:field="form" method="post">
			<g:FlowPanel ui:field="content" styleName="{css.content}"></g:FlowPanel>
		</g:FormPanel>
	</g:FlowPanel>
</ui:UiBinder> 