<module rename-to='commons'>
	
	<inherits name='com.google.gwt.user.User' />
	<inherits name="com.google.gwt.i18n.I18N" />
	<inherits name='com.google.gwt.json.JSON'/>
	
	<inherits name="com.bramosystems.oss.player.core.Core"/>
	<inherits name="com.bramosystems.oss.player.uibinder.UiBinder"/>
	
	<inherits name='com.paraschool.editor.api.EditorApi' />
	<inherits name='com.paraschool.htmllist.HTMLList' />
	
	<extend-property name='locale' values='fr'/>
	<set-property name="compiler.stackMode" value="strip"/>
	<source path='client' />
</module>
